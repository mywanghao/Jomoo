        android:layout_height="wrap_content"
        android:layout_marginLeft="60dp"
        android:layout_marginRight="60dp"
        android:layout_marginTop="40dp"
        android:layout_marginBottom="40dp"
        />


    <include layout="@layout/view_numinput" />

